type VECT = array [1..3] of integer ;
VECTPTR = ^VECT;
procedure SUB1;
var A, B: VECT;
P, Q: VECTPTR;
begin
A[1] := 7 ; A[2] := 8 ; A[3] := 9 ;
B[1] := 7 ; B[2] := 8 ; B[3] := 9 ;
P := @A; Q := @B;
SUB2(P, Q) ;
end ;
procedure SUB2(R: VECTPTR; var S : VECTPTR) ;
begin
R^[1] := R^[1] + 10;    // 1 -> A[1] 
S^[1] := S^[1] + 10;    // 2 -> B[1]
if ... then R := S ;    // 3 -> R 
else S := R;            // 4 -> S
end ;

